{
  "name": "@structured-fit/js-api-definitions",
  "version": "0.0.1",
  "description": "Public API definitions for use with Structured Fitness",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc ava",
    "preyalc": "tsc"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/structured-fit/js-api-definitions.git"
  },
  "author": "Structured Fitness (https://structured.fit/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/structured-fit/js-api-definitions/issues"
  },
  "homepage": "https://github.com/structured-fit/js-api-definitions#readme",
  "devDependencies": {
    "@types/node": "^12.0.8",
    "ava": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.7.1",
    "nyc": "^14.1.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "class-validator": "^0.9.1"
  }
}
